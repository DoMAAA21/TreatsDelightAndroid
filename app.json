{
  "expo": {
    "name": "TreatsDelightAndroid",
    "slug": "TreatsDelightAndroid",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/capstone_logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/capstone_logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#21a86d"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSPhotoLibraryAddUsageDescription": "For Saving Receipt"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/capstone_icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "WRITE_EXTERNAL_STORAGE"
      ],
      "package": "com.domaaa21.TreatsDelightAndroid"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ]
    ]

  }
}
