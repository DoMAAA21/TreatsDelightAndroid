{
  "expo": {
    "name": "TreatsDelightAndroid",
    "slug": "TreatsDelightAndroid",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/capstone_logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/capstone_logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#21a86d"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/capstone_logo.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      ["expo-build-properties", {
        "android": {
          "usesCleartextTraffic": true
        }
      }
      ]
    ]
  }
}
